{
  "name": "wiktoria-lars-pi-exhibition",
  "version": "2.0.0-pi",
  "packageManager": "npm@10.2.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && NODE_OPTIONS=\"--max-old-space-size=2048\" next build",
    "build:pi": "rm -rf .next && NODE_OPTIONS=\"--max-old-space-size=1024\" next build",
    "start": "next start",
    "start:pi": "NODE_OPTIONS=\"--max-old-space-size=1024\" next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.1",
    "lucide-react": "^0.441.0",
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18",
    "ultravox-client": "^0.3.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ws": "^8.5.12",
    "eslint": "^8",
    "eslint-config-next": "13.5.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "pi-notes": {
    "architecture": "ARM64",
    "nextjs-version": "13.5.6 for ARM compatibility",
    "memory-limit": "1024MB for Pi 4",
    "babel-required": "SWC not available on ARM"
  }
}